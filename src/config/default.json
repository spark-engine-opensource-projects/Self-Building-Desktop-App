    "healthCheckInterval": 10000,
    "maxLogFileSize": 10485760,
    "maxLogFiles": 5,
    "enableMetrics": true
  },
  "ai": {
    "model": "claude-3-5-sonnet-20241022",
    "temperature": 0.7,
    "maxTokens": 2000,
    "enableCodeValidation": true
  },
  "ui": {
    "enableDevTools": false,
    "autoSavePrompts": true,
    "maxHistoryItems": 50
  }
}